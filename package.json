{
  "name": "tricking",
  "version": "0.1.0",
  "private": true,
  "repository" :
  { "type" : "git"
  , "url" : "https://github.com/mantism/TrickingWiki"
  },
  "dependencies": {
    "d3-array": "^1.2.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "react-tooltip": "^3.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/styles.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "d3-geo": "^1.6.4",
    "d3-selection": "^1.1.0",
    "gh-pages": "^1.0.0",
    "lodash": "^4.17.4"
  },
  "homepage": "mantism.github.io/TrickingWiki"
}
